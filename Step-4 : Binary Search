
1. Binary Search

 public:
    int binarysearch(int arr[], int n, int k) {
        int l=0,r=n-1;
        int mid = 0;
        while(l<=r){
            mid = l + (r-l)/2;
            if(arr[mid] == k){
                return mid;
            }
            else if(arr[mid]>k){
                r=mid-1;
            }
            else {
                l=mid+1;
            }
        }
        return -1;
    }
